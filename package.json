{
  "name": "pixiviz",
  "version": "1.31.3",
  "scripts": {
    "serve": "node ./tools/sensitiveWordsGen.js && vue-cli-service serve",
    "build": "node ./tools/processVersion.js && set NODE_OPTIONS=--openssl-legacy-provider && vue-cli-service build",
    "test:e2e": "vue-cli-service test:e2e",
    "analyze": "cross-env ANALYZE=true vue-cli-service build",
    "dev": "npm run serve",
    "dev:built": "npm run build && http-server ./dist -b",
    "gen:anno": "cross-env NODE_ENV=prod node ./tools/announcementGen.js",
    "gen:anno-dev": "cross-env NODE_ENV=dev node ./tools/announcementGen.js",
    "gen:proxy": "node ./tools/proxyConfigGen.js",
    "gen:ver": "node ./tools/processVersion.js",
    "gen:sensitive": "node ./tools/sensitiveWordsGen.js",
    "push:anno": "tigo cfs deploy pixiviz-anno-prod",
    "push:anno-dev": "tigo cfs deploy pixiviz-anno-dev",
    "push:proxy": "cross-env NODE_ENV=prod node ./tools/proxyConfigUpload.js",
    "push:proxy-dev": "cross-env NODE_ENV=dev node ./tools/proxyConfigUpload.js",
    "push:config": "tigo cfs deploy pixiviz-prod",
    "start": "npm run serve",
    "upload:anno": "npm run push:anno",
    "upload:anno-dev": "npm run push:anno-dev",
    "upload:proxy": "npm run push:proxy",
    "upload:proxy-dev": "npm run push:proxy-dev",
    "compress-svg": "npx svgo ./src/assets/icons",
    "lint": "eslint ./src --ext=.js,.vue",
    "lint-fix": "eslint ./src --ext=.js,.vue --fix"
  },
  "dependencies": {
    "@better-scroll/core": "^2.5.1",
    "@pwp-app/better-element-ui": "^2.15.7",
    "@pwp-app/vue-context-menu": "^1.3.1",
    "@pwp-app/vue-lazyload": "^1.3.4",
    "axios": "^0.24.0",
    "clipboard-polyfill": "^3.0.3",
    "core-js": "^3.33.3",
    "dayjs": "^1.11.10",
    "hash-wasm": "^4.11.0",
    "jszip": "^3.10.1",
    "level-js": "^6.1.0",
    "levelup": "^5.1.1",
    "lodash-es": "^4.17.21",
    "nanoid": "^3.3.7",
    "pixland": "^0.1.3",
    "qrcode-svg": "^1.1.0",
    "qs": "^6.11.2",
    "regex-go": "^0.4.0",
    "register-service-worker": "^1.7.2",
    "seedrandom": "^3.0.5",
    "share-token": "^0.1.1",
    "vue": "^2.7.15",
    "vue-cookies": "^1.8.3",
    "vue-infinite-loading": "^2.4.5",
    "vue-router": "^3.6.5",
    "vue-share-popup": "^1.0.0",
    "vue2-touch-events": "^3.2.3",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.23.3",
    "@gfx/zopfli": "^1.0.15",
    "@vue/cli-plugin-babel": "^4.5.19",
    "@vue/cli-plugin-e2e-cypress": "^4.5.19",
    "@vue/cli-plugin-pwa": "^4.5.19",
    "@vue/cli-plugin-router": "^4.5.19",
    "@vue/cli-plugin-vuex": "^4.5.19",
    "@vue/cli-service": "^4.5.19",
    "babel-plugin-component": "^1.1.1",
    "brotli-webpack-plugin": "^1.1.0",
    "compression-webpack-plugin": "^6.1.1",
    "cross-env": "^7.0.3",
    "cypress": "^6.9.1",
    "eslint": "^8.54.0",
    "eslint-config-alloy": "^5.1.2",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "html-webpack-plugin": "^4.5.2",
    "jindan-webpack-plugin": "^0.2.6",
    "less": "^3.13.1",
    "less-loader": "^6.2.0",
    "prettier": "^3.1.0",
    "vue-cli-plugin-element": "^1.0.1",
    "vue-template-compiler": "^2.7.15"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "last 3 Android versions",
    "last 3 iOS versions",
    "not IE 11"
  ]
}
