{
  "name": "pixiviz",
  "version": "1.16.1-f4",
  "private": true,
  "scripts": {
    "start": "npm run serve",
    "dev": "npm run serve",
    "dev:built": "npm run build && http-server ./dist -b",
    "serve": "node ./tools/processVersion.js && vue-cli-service serve",
    "build": "node ./tools/processVersion.js && vue-cli-service build",
    "analyze": "cross-env ANALYZE=true vue-cli-service build",
    "gen:ver": "node ./tools/processVersion.js",
    "gen:anno": "cross-env NODE_ENV=prod node ./tools/announcementGen.js",
    "gen:anno-dev": "cross-env NODE_ENV=dev node ./tools/announcementGen.js",
    "gen:proxy": "node ./tools/proxyConfigGen.js",
    "upload:anno": "npm run push:anno",
    "upload:anno-dev": "npm run push:anno-dev",
    "upload:proxy": "npm run push:proxy",
    "upload:proxy-dev": "npm run push:proxy-dev",
    "push:anno": "cross-env NODE_ENV=prod node ./tools/announcementUpload.js",
    "push:anno-dev": "cross-env NODE_ENV=dev node ./tools/announcementUpload.js",
    "push:proxy": "cross-env NODE_ENV=prod node ./tools/proxyConfigUpload.js",
    "push:proxy-dev": "cross-env NODE_ENV=dev node ./tools/proxyConfigUpload.js"
  },
  "dependencies": {
    "@pwp-app/better-element-ui": "^2.14.0-fix-1",
    "@pwp-app/vue-lazyload": "^1.3.3-fix-1",
    "axios": "^0.21.0",
    "clipboard-polyfill": "^3.0.1",
    "core-js": "^3.7.0",
    "dayjs": "^1.9.6",
    "qs": "^6.9.4",
    "register-service-worker": "^1.7.1",
    "vue": "^2.6.12",
    "vue-cookies": "^1.7.4",
    "vue-infinite-loading": "^2.4.5",
    "vue-meta": "^2.4.0",
    "vue-router": "^3.4.9",
    "vue-touch": "^2.0.0-beta.4",
    "vue2-touch-events": "^2.3.2",
    "vuex": "^3.5.1"
  },
  "devDependencies": {
    "@gfx/zopfli": "^1.0.15",
    "@vue/cli-plugin-babel": "^4.5.9",
    "@vue/cli-plugin-pwa": "^4.5.9",
    "@vue/cli-plugin-router": "^4.5.9",
    "@vue/cli-plugin-vuex": "^4.5.9",
    "@vue/cli-service": "^4.5.9",
    "babel-plugin-component": "^1.1.1",
    "brotli-webpack-plugin": "^1.1.0",
    "compression-webpack-plugin": "^6.1.1",
    "cross-env": "^7.0.2",
    "image-webpack-loader": "^6.0.0",
    "less": "^3.12.2",
    "less-loader": "^6.2.0",
    "qiniu": "^7.3.2",
    "vue-cli-plugin-element": "^1.0.1",
    "vue-template-compiler": "^2.6.12"
  },
  "browserslist": [
    "> 1%",
    "last 1 versions",
    "last 4 Android versions",
    "last 3 iOS versions"
  ]
}
